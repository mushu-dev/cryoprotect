{
  "name": "cryoprotect-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:with-convex": "NEXT_PUBLIC_USE_CONVEX=true next dev",
    "build": "next build",
    "build:with-convex": "NEXT_PUBLIC_USE_CONVEX=true next build",
    "export": "next export",
    "build-export": "next build",
    "start": "next start",
    "validate-netlify": "node validate-netlify-build.js",
    "validate-analytics": "node validate-analytics.js",
    "convex:codegen": "node scripts/generate-convex-types.js",
    "convex:push": "convex push",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "convex:status": "convex status",
    "deploy:with-convex": "NEXT_PUBLIC_USE_CONVEX=true npm run build && npm run export",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:component": "jest --testMatch='**/*.test.js' --testPathIgnorePatterns='e2e|playwright'",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:snapshot": "jest --updateSnapshot",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:accessibility": "playwright test accessibility.spec.js",
    "test:visual": "bash run-visual-tests.sh",
    "test:forms": "playwright test experiment-form-validation.spec.js",
    "test:performance": "playwright test performance-metrics.spec.js",
    "test:security": "playwright test security-testing.spec.js",
    "test:load": "playwright test load-testing.spec.js",
    "test:cross-browser": "playwright test cross-browser-compatibility.spec.js --project=chromium,firefox,webkit",
    "test:security-performance": "bash run-security-performance-tests.sh",
    "test:visualization": "playwright test data-visualization.spec.js",
    "test:filtering": "playwright test data-filtering.spec.js",
    "test:comparison": "playwright test data-comparison.spec.js",
    "test:data-features": "playwright test data-visualization.spec.js data-filtering.spec.js data-comparison.spec.js",
    "test:user-flow": "playwright test tests/e2e/user-flow.spec.js",
    "test:data-integrity": "playwright test tests/e2e/data-integrity.spec.js",
    "test:experimental-data": "playwright test tests/e2e/experimental-data.spec.js",
    "test:data-analysis": "playwright test tests/e2e/data-analysis.spec.js",
    "test:experimental-features": "playwright test tests/e2e/experimental-data.spec.js tests/e2e/data-analysis.spec.js",
    "test:experimental-features:ui": "playwright test tests/e2e/experimental-data.spec.js tests/e2e/data-analysis.spec.js --ui",
    "test:experimental": "bash run-experimental-tests.sh",
    "test:user-data-integrity": "bash run-data-integrity-tests.sh",
    "test:convex": "NEXT_PUBLIC_USE_CONVEX=true playwright test tests/e2e/convex-integration.spec.js",
    "test:analytics": "node validate-analytics.js",
    "test:protein-visualizer": "jest tests/protein-visualizer",
    "test:protein-visualizer:e2e": "playwright test tests/protein-visualizer",
    "test:protein-visualizer:manual": "./run-protein-visualizer-tests.sh",
    "test:api:molecule-mixture": "./tests/api/run-api-tests.sh",
    "test:api:verify": "./tests/api/run-manual-tests.sh",
    "test:api:live": "./tests/api/run-live-tests.sh",
    "test:all": "bash run-all-tests.sh",
    "verify-deployment": "node scripts/verify-netlify-deployment.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@hookform/resolvers": "^3.3.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tailwindcss/forms": "^0.5.6",
    "axios": "^1.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "convex": "^1.3.1",
    "date-fns": "^2.30.0",
    "formik": "^2.4.5",
    "lucide-react": "^0.284.0",
    "molstar": "^4.15.0",
    "next": "13.5.4",
    "next-auth": "^4.23.2",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-day-picker": "^8.8.2",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.7.0",
    "yup": "^1.3.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.7.1",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@netlify/plugin-nextjs": "^4.41.3",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.16",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.51.0",
    "eslint-config-next": "13.5.4",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.31"
  },
  "plugins": [
    {
      "package": "@netlify/plugin-nextjs"
    }
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@/components/(.*)$": "<rootDir>/src/components/$1",
      "^@/features/(.*)$": "<rootDir>/src/features/$1",
      "^@/hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^@/services/(.*)$": "<rootDir>/src/services/$1",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
